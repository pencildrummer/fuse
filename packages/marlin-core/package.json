{
  "name": "@fuse-labs/marlin-core",
  "version": "1.0.0",
  "description": "Core plugin for FUSE for Marlin-based devices",
  "private": true,
  "author": "Fabio Borella",
  "license": "ISC",
  "type": "module",
  "files": [
    "dist",
    "dist-client",
    "lib/server"
  ],
  "main": "./lib/server/index.js",
  "exports": {
    "./server": "./lib/server/index.js",
    "./client": {
      "require": "./dist-client/marlin-core.umd.js",
      "import": "./dist-client/marlin-core.es.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "vite build",
    "dev": "vite build --watch",
    "preview": "vite preview",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.1.0",
    "chalk": "^5.0.1",
    "fs-extra": "^10.0.1",
    "gcode-parser": "^1.3.7",
    "is-electron": "^2.2.1",
    "serialport": "^10.4.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "vite": "^2.8.0"
  },
  "fuse": {
    "title": "[Marlin] Core",
    "type": "driver",
    "devices": [
      "fdm_printer"
