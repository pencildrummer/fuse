{
  "name": "@fuse-labs/marlin-gcode-viewer",
  "version": "1.0.0",
  "description": "GCode viewer plugin for FUSE for Marlin-based devices",
  "type": "module",
  "private": true,
  "files": [
    "dist-client"
  ],
  "main": "./dist-client/marlin-gcode-viewer.umd.js",
  "module": "./dist-client/marlin-gcode-viewer.es.js",
  "exports": {
    "./client": {
      "require": "./dist-client/marlin-gcode-viewer.umd.js",
      "import": "./dist-client/marlin-gcode-viewer.es.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "preview": "vite preview",
    "clean": "rm -rf node_modules"
  },
  "author": "Fabio Borella",
  "license": "ISC",
  "fuse": {
    "title": "[Marlin] GCode Viewer",
    "pagesUrl": "gcode-viewer",
    "icon": "CodeIcon",
    "tooltip": "GCode monitor",
    "devices": [
      "fdm_printer"
    ]
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "vite": "^2.8.0"
  },
  "dependencies": {
    "@radix-ui/react-aspect-ratio": "^0.1.4"
  }
}
